<div class='title_container'>
  <h2 class="title_welcome">The latest {{#each leagues}}<span>{{this}}</span>{{/each}} and <span>{{fighterName}}
      videos</span></h2>
</div>
{{#each videos}}
<div style="width:100%;height:100%;width: 820px; height: 461.25px; float: none; clear: both; margin: 2px auto;">
  <h2 class="video_title">{{this.title}}</h2>
  <embed src="http://www.youtube.com/v/{{this.id}}" wmode="transparent" type="application/x-shockwave-flash"
    width="200px" height="200px" allowfullscreen="true" title="Adobe Flash Player" class='video_item'>
  <p class='video_description'>{{this.description}}</p>
</div>
{{/each}}